    <!-- Question B01 -->
    <string name="b01">Why is physical activity important for your health?</string>
    <string name="b01A">It helps you get good grades in school.</string>
    <string name="b01B">It keeps your body strong and helps prevent diseases.</string> <!-- Correct Answer -->
    <string name="b01C">It makes you hungry all the time.</string>
    <string name="b01D">It doesn’t really matter for your health.</string>

    <!-- Question B02 -->
    <string name="b02">How much physical activity should children do each day?</string>
    <string name="b02A">10 minutes.</string>
    <string name="b02B">30 minutes.</string>
    <string name="b02C">At least 60 minutes.</string> <!-- Correct Answer -->
    <string name="b02D">5 hours.</string>

    <!-- Question B03 -->
    <string name="b03">Which of these is considered a physical activity?</string>
    <string name="b03A">Watching TV.</string>
    <string name="b03B">Playing video games.</string>
    <string name="b03C">Running, jumping, or playing sports.</string> <!-- Correct Answer -->
    <string name="b03D">Eating snacks.</string>

    <!-- Question B04 -->
    <string name="b04">How can physical activity help prevent non-communicable diseases (NCDs)?</string>
    <string name="b04A">By making you sick more often.</string>
    <string name="b04B">By keeping your heart, lungs, and body healthy.</string> <!-- Correct Answer -->
    <string name="b04C">By making you spend more time sitting.</string>
    <string name="b04D">By helping you avoid exercise.</string>

    <!-- Question B05 -->
    <string name="b05">Which of the following is a fun way to stay active?</string>
    <string name="b05A">Playing outside with friends.</string> <!-- Correct Answer -->
    <string name="b05B">Watching TV all day.</string>
    <string name="b05C">Sleeping late every day.</string>
    <string name="b05D">Eating fast food.</string>

    <!-- Question B06 -->
    <string name="b06">Which of these activities can you easily include in your daily routine?</string>
    <string name="b06A">Taking the stairs instead of the elevator.</string> <!-- Correct Answer -->
    <string name="b06B">Sitting on the couch all day.</string>
    <string name="b06C">Playing video games all evening.</string>
    <string name="b06D">Skipping your physical education class.</string>

    <!-- Question B07 -->
    <string name="b07">How can you stay active even when you have a busy schedule?</string>
    <string name="b07A">Skip physical activity altogether.</string>
    <string name="b07B">Find small opportunities to move, like walking to school.</string> <!-- Correct Answer -->
    <string name="b07C">Only exercise on weekends.</string>
    <string name="b07D">Ignore physical activity until you have more time.</string>


