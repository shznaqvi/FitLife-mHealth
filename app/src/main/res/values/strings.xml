    <string name="c01B">Including a variety of different foods in the right amounts.</string> <!-- Correct Answer -->
    <string name="c01C">Eating as much as you want of your favourite foods.</string>
    <string name="c01D">Avoiding all fats and carbohydrates.</string>

    <!-- Question C02 -->
    <string name="c02">What is a healthy snack choice?</string>
    <string name="c02A">Potato chips.</string>
    <string name="c02B">Candy bars.</string>
    <string name="c02C">Fresh fruit or yogurt.</string> <!-- Correct Answer -->
    <string name="c02D">Cookies and soda.</string>

    <!-- Question C03 -->
