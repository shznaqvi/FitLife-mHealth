    <!-- Question A01 -->
    <string name="a01">What are Non-Communicable Diseases (NCDs)?</string>
    <string name="a01A">Diseases that spread from one person to another</string>
    <string name="a01B">Diseases caused by germs or infections</string>
    <string name="a01C">Long-lasting diseases that do not spread from person to person</string> <!-- Correct Answer -->
    <string name="a01D">Diseases that only affect animals</string>

    <!-- Question A02 -->
    <string name="a02">Why is it important to prevent NCDs?</string>
    <string name="a02A">NCDs can be easily treated</string>
    <string name="a02B">Preventing NCDs can help people live better and save money on healthcare</string> <!-- Correct Answer -->
    <string name="a02C">NCDs cannot be prevented</string>
    <string name="a02D">NCDs only affect older people</string>

    <!-- Question A03 -->
    <string name="a03">What can help reduce the risk of NCDs?</string>
    <string name="a03A">Being physically active</string> <!-- Correct Answer -->
    <string name="a03B">Eating lots of junk food</string>
    <string name="a03C">Smoking cigarettes</string>
    <string name="a03D">Skipping health check-ups</string>

    <!-- Question A04 -->
    <string name="a04">What impact do NCDs have on a person’s life?</string>
    <string name="a04A">They have a small impact and are easy to manage</string>
    <string name="a04B">They can cause serious health problems, lower quality of life, and early death</string> <!-- Correct Answer -->
    <string name="a04C">They make people healthier</string>
    <string name="a04D">They only affect physical health, not mental health</string>

    <!-- Question A05 (Select all that apply) -->
    <string name="a05">What are some effective ways to prevent NCDs? (Select all that apply)</string>
    <string name="a05A">Eating a healthy diet</string> <!-- Correct Answer -->
    <string name="a05B">Exercising regularly</string> <!-- Correct Answer -->
    <string name="a05C">Avoiding tobacco and excessive alcohol</string> <!-- Correct Answer -->
    <string name="a05D">Ignoring signs of illness</string>

    <!-- Question A06 -->
    <string name="a06">Which of these is NOT a changeable risk factor for NCDs?</string>
    <string name="a06A">Family history of disease</string> <!-- Correct Answer -->
    <string name="a06B">Smoking</string>
    <string name="a06C">Lack of exercise</string>
    <string name="a06D">Unhealthy diet</string>
